<div class="row">
  <div class="col-md-4">
    <p class="lead"><span class="glyphicon glyphicon-cog"></span> Lists</p>
  </div>
  <div class="col-md-8">
    <form role="form">
      <legend>This is form</legend>
      <div class="form-group">
        <label for="exampleInputEmail1">Email address</label>
        <!--{{view Ember.TextField valueBinding=someValue}}-->
        {{input type=text}}
      </div>
      <button type="submit" class="btn btn-default">Yaaarr!</button>
    </form> 
    <h3>Achievements</h3>
    <hr>

    <ul class="achievements">
      {{#each controller}}
      {{else}}
      <li>
        <h3>There are no achievements yet.</h3>
      </li>
      {{/each}}
    </ul>
  </div>
</div>